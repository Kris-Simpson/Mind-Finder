$('#submenu').empty().append("<%= j render 'shared/submenu' %>").tree();

<% if current_user.tests.any? %>
  $('#created_tests').empty().append('<%= j render "shared/content_table", content: current_user.tests.page(params[:page]), model_name: "tests" %>');
  
  $('.information').popover();
  $('.btn, .action').tooltip();
<% else %>
  $('.form-search').slideUp(200);
  $('.row-fluid .span6 h4').slideDown(200);
  
  $('#created_tests').slideUp(400).delay(400).empty();
<% end %>