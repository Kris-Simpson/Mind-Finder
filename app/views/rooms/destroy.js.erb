$("#room_<%= @room.id %>").slideUp(200);
setTimeout(function() {
  $("#room_<%= @room.id %>").remove();
  var rooms = $('.block');
  if(rooms.length == 0) {
    $('div#created_rooms .empty-blocks').append('There are no rooms').hide().slideDown();
  }
}, 200);

$('#submenu').empty().append("<%= j render 'shared/submenu' %>").tree();

<% unless current_user.rooms.all.present? %>
  $('#left .empty-menu').append('There are no <em>rooms</em> or <em>tests</em>');
<% end %>