<div class="room_block" id="room_<%= room.id %>">
  <%= link_to room.name, room, class: "room_name", remote: true %>
  <%= link_to 'Edit', edit_room_path(room), class: "edit_room_link", remote: true %>

  <% unless room.is_main == true %>
    <%= link_to 'Destroy', room, method: :delete, data: { confirm: 'Are you sure you want to delete this room?' }, class: "destroy_room_link", remote: true %>
  <% end %>
</div>